{
    "name": "@smatch-corp/react-icons",
    "version": "1.25.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "sideEffects": false,
    "type": "module",
    "files": [
        "package.json",
        "README.md",
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        }
    },
    "peerDependencies": {
        "react": "^17 || ^18"
    },
    "devDependencies": {
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.5.7",
        "@types/react": "^18.2.21",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.48.0",
        "eslint-plugin-dprint-integration": "^0.3.0",
        "eslint-plugin-perfectionist": "^1.5.1",
        "fs-extra": "^11.1.1",
        "msw": "0.0.0-fetch.rc-17",
        "react": "^18.2.0",
        "release-it": "^16.1.5",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vitest": "^0.34.3"
    },
    "scripts": {
        "test": "vitest",
        "import": "pnpm tsx script/index.ts && pnpm eslint --fix src",
        "import:force": "pnpm run import",
        "build": "unbuild",
        "prepack": "pnpm build"
    },
    "packageManager": "pnpm@8.6.0",
    "dependencies": {
        "unbuild": "^2.0.0"
    }
}
